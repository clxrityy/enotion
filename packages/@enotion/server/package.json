{
  "name": "@enotion/server",
  "version": "0.0.0",
  "type": "module",
  "private": false,
  "description": "Backend services and APIs for accessing and managing server-side functionalities",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./system": {
      "import": "./dist/system/index.js",
      "types": "./dist/system/index.d.ts"
    },
    "./network": {
      "import": "./dist/network/index.js",
      "types": "./dist/network/index.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "backend",
    "api",
    "server",
    "rest",
    "nextjs",
    "child-process",
    "server-side",
    "util",
    "os",
    "file-system",
    "fs",
    "path",
    "http",
    "https",
    "middleware",
    "routing"
  ],
  "author": {
    "name": "MJ Anglin",
    "email": "contact@mjanglin.com",
    "url": "https://mjanglin.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.18.3",
  "dependencies": {
    "@enotion/config": "workspace:*",
    "@enotion/core": "workspace:*"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "jest": "^30.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/clxrityy/enotion/issues",
    "email": "contact@mjanglin.com"
  },
  "homepage": "https://github.com/clxrityy/enotion/tree/main/packages/%40enotion/server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clxrityy/enotion.git"
  }
}