{
  "name": "@enotion/components",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "description": "A collection of reusable React components to simplify your development process, and ease common tasks",
  "main": "dist/index.js",
  "types": "dist/index.d.mts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.mts"
    },
    "./index.css": {
      "default": "./dist/index.css"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "nextjs",
    "components",
    "typescript",
    "frontend",
    "ui",
    "design-system",
    "component-library",
    "reusable-components",
    "custom-components",
    "buttons",
    "modals",
    "forms",
    "cards",
    "navigation",
    "layout",
    "theming",
    "responsive",
    "accessibility"
  ],
  "author": {
    "name": "MJ Anglin",
    "email": "contact@mjanglin.com",
    "url": "https://mjanglin.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.18.3",
  "dependencies": {
    "@enotion/config": "workspace:*",
    "@enotion/core": "workspace:*",
    "@enotion/hooks": "workspace:*",
    "@enotion/notify": "workspace:*"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "jest": "^30.2.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.14",
    "terser": "^5.44.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/clxrityy/enotion/issues",
    "email": "contact@mjanglin.com"
  },
  "homepage": "https://github.com/clxrityy/enotion/tree/main/packages/%40enotion/components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clxrityy/enotion.git"
  }
}
