{
  "name": "@enotion/core",
  "version": "0.1.0",
  "description": "Core package for enotion",
  "private": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist && rm -rf .turbo && rm -rf node_modules",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "exports": {
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "clsx": "^2.1.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "terser": "^5.44.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@types/node": "^24.6.1",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@enotion/config": "workspace:^"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clxrityy/enotion.git"
  },
  "bugs": {
    "url": "https://github.com/clxrityy/enotion/issues"
  },
  "homepage": "https://github.com/clxrityy/enotion#readme",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.18.3"
}
