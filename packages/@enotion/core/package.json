{
  "name": "@enotion/core",
  "version": "0.1.0",
  "description": "Core package for enotion",
  "private": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist && rm -rf .turbo && rm -rf node_modules"
  },
  "exports": {
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs",
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./constants": {
      "import": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts",
      "require": "./dist/constants/index.cjs",
      "default": "./dist/constants/index.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "terser": "^5.44.0",
    "ts-jest": "^29.4.4",
    "tsup": "^8.5.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@types/node": "^24.6.1",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@enotion/config": "workspace:^"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clxrityy/enotion.git"
  },
  "bugs": {
    "url": "https://github.com/clxrityy/enotion/issues"
  },
  "homepage": "https://github.com/clxrityy/enotion#readme",
  "publishConfig": {
    "access": "public"
  }
}
